            android:layout_width="0dp"
            android:layout_height="180dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/onboard_area8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/some_id"
            app:layout_marginEnd="40dp"
            app:layout_marginStart="40dp" />

        <ImageView
            android:id="@+id/list1"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="18dp"
            android:src="@drawable/list1"
            app:layout_constraintEnd_toEndOf="@+id/customBackground"
